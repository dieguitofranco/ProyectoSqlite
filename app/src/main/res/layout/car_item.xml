<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imgCar"
        android:layout_width="277dp"
        android:layout_height="187dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/carros"
        tools:layout_editor_absoluteX="73dp" />

    <TextView
        android:id="@+id/textCodigo"
        android:layout_width="286dp"
        android:layout_height="32dp"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@+id/imgCar"
        tools:layout_editor_absoluteX="73dp" />

    <TextView
        android:id="@+id/textColor"
        android:layout_width="286dp"
        android:layout_height="32dp"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/textModelo"
        tools:layout_editor_absoluteX="73dp" />

    <TextView
        android:id="@+id/textModelo"
        android:layout_width="286dp"
        android:layout_height="32dp"
        android:layout_marginTop="21dp"
        app:layout_constraintTop_toBottomOf="@+id/textPlaca"
        tools:layout_editor_absoluteX="73dp" />

    <TextView
        android:id="@+id/textPlaca"
        android:layout_width="286dp"
        android:layout_height="32dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textPropietario"
        tools:layout_editor_absoluteX="73dp" />

    <TextView
        android:id="@+id/textTipo"
        android:layout_width="286dp"
        android:layout_height="32dp"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/textColor"
        tools:layout_editor_absoluteX="73dp" />

    <TextView
        android:id="@+id/textValue"
        android:layout_width="286dp"
        android:layout_height="32dp"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="572dp" />

    <TextView
        android:id="@+id/textidentificacion"
        android:layout_width="286dp"
        android:layout_height="32dp"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="619dp" />

    <TextView
        android:id="@+id/textUrl"
        android:layout_width="286dp"
        android:layout_height="32dp"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="665dp" />

    <TextView
        android:id="@+id/textPropietario"
        android:layout_width="286dp"
        android:layout_height="32dp"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/textCodigo"
        tools:layout_editor_absoluteX="73dp" />
</androidx.constraintlayout.widget.ConstraintLayout>